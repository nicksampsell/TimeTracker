<Window x:Class="TimeTracker.ProjectEditForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TimeTracker" 
        xmlns:uc="clr-namespace:TimeTracker.Controls"
        mc:Ignorable="d"
        Title="Edit Project : Time Tracker" Height="350" Width="470">
    <StackPanel Margin="10">
        <TextBlock Style="{StaticResource Header}">Edit Project</TextBlock>
        <uc:ProjectForm x:Name="editForm" OnButtonClick="Button_Click" ButtonText="Save Changes"></uc:ProjectForm>
    </StackPanel>
</Window>
